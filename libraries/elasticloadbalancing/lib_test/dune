(jbuild_version 1)

(executables
 ((names (test_async test_lwt))
  (flags (:standard -w -27))
  (modules (test_async test_lwt aws_elasticloadbalancing_test))
  (libraries (aws aws_elasticloadbalancing
              aws-async aws-lwt
              oUnit yojson
              async cohttp-async
              lwt cohttp-lwt cohttp-lwt-unix))))

(alias
 ((name runtest)
  (deps (test_async.exe))
  (action (run ${<}))))

(alias
 ((name runtest)
  (deps (test_lwt.exe))
  (action (run ${<}))))
